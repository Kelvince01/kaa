{
  "name": "api",
  "version": "1.0.50",
  "description": "API for Kaa SaaS",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "check-types-": "bun --max-old-space-size=4096 --bun tsc --noEmit",
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev-hot": "bun run --hot src/index.ts",
    "start:prod:dist": "bun run dist/index.js",
    "start": "bun src/index.ts",
    "start:prod": "bun run server",
    "seed": "bun src/scripts/seed/index.ts",
    "seed:rbac": "bun src/scripts/seeds/rbac/index.ts",
    "seed:templates": "bun src/scripts/seeds/templates/index.ts",
    "lint": "bun x biome check .",
    "lint:format": "bun x biome format .",
    "lint:fix": "bun x biome check --write .",
    "format": "biome format --write ./src",
    "clean": "rm -rf dist node_modules",
    "security-scan": "bun audit --production && gitleaks detect",
    "//": "New commands",
    "build:client": "bun x vite build",
    "build:ssr": "bun x vite build --mode ssr",
    "build:new": "bun run build:client && bun run build:ssr",
    "generate": "bun run scripts/generateStaticPages.ts",
    "dev:new": "concurrently --kill-others-on-fail --success first -n bun,vite -c blue,green \"cross-env NODE_ENV=development bun --hot run src/index.ts\" \"cross-env NODE_ENV=development vite\"",
    "start:new": "cross-env NODE_ENV=production bun run src/index.ts",
    "loadtest:login": "bun run scripts/loadtest.ts",
    "loadtest:homepage": "bun run scripts/homepagetest.ts",
    "artillery:login": "npx artillery run scripts/artillery-login.yml",
    "artillery:homepage": "npx artillery run scripts/artillery-homepage.yml"
  },
  "dependencies": {
    "@ai-sdk/google": "catalog:",
    "@ai-sdk/openai": "catalog:",
    "@bogeychan/elysia-etag": "^0.1.0",
    "@bogeychan/elysia-logger": "^0.1.10",
